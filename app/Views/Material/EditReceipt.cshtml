            var url = '/Material/UpdateMaterialType';
            $.ajax({
                url: "/Material/CheckDuplicatedMaterialName/",
                type: "POST",
                contentType: "application/json;charset=UTF-8",
                dataType: "json",
                data: `{'MaterialTypeName':'${data.MaterialTypeName}', 'id':'${data.Id}'}`,
                success: function (result) {
                    if (result == true) {
                        $.post(url, data,
                            function (rs) {
                                if (rs == 1) {
