                                <td>
                                    <input type="number" disabled class="form-control inputText sl_tongMH2" value="${appendix.sl_tong == null ? '' : appendix.sl_tong.trim()}" id="sltongMH2_${index + 1}"/>
                                </td>
                                <td>
                                    <input type="text" disabled class="form-control inputText don_vi_tinhMH2" value="${appendix.don_vi_tinh == null ? '' : appendix.don_vi_tinh}" id="donvitinhMH2_${index + 1}"/>
                                </td>
                                <td>
                                    <input type="number" disabled class="form-control inputText quy_kyMH2" value="${appendix.quy_ky == null ? '' : appendix.quy_ky.trim()}" id="quykyMH2_${index + 1}"/>
                                </td>
                                <td>
                                    <input type="text" disabled class="form-control inputText muc_dich_sdMH2" value="${appendix.muc_dich_sd == null ? '' : appendix.muc_dich_sd}" id="mucdichsdMH2_${index + 1}"/>
